<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed"
          xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
          namespace="Orm\Zed\LiselRequest\Persistence"
          package="src.Orm.Zed.LiselRequest.Persistence">

    <table name="nxs_lisel_request" idMethod="native" allowPkInsert="true" identifierQuoting="true">
        <column name="id_nxs_lisel_request" required="true" type="INTEGER" primaryKey="true" autoIncrement="true"/>
        <column name="message" required="true" type="LONGVARCHAR"/>
        <column name="response" type="LONGVARCHAR"/>
        <column name="processed" type="TIMESTAMP"/>
        <column name="request_ident" type="VARCHAR"/>
        <column name="created" type="TIMESTAMP" defaultExpr="CURRENT_TIMESTAMP"/>
        <id-method-parameter value="nxs_lisel_request_pk_seq"/>
    </table>

</database>
